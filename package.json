{
  "name": "@dannyman/use-store",
  "version": "2.0.9",
  "description": "Custom React Hook that enables a global Store.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "prebuild": "rm -rf dist/",
