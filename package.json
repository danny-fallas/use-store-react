  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@rollup/plugin-url": "^8.0.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "nodemon": "^2.0.20",
    "rollup": "^3.2.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
